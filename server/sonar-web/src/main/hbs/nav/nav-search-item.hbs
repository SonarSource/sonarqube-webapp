{{#if extra}}
  {{#gt index 0}}
    <div class="divider"></div>
  {{/gt}}
  <div class="dropdown-header">{{extra}}</div>
{{/if}}

<a href="{{url}}" title="{{name}}">
  {{#if q}}{{qualifierIcon q}}{{/if}}
  {{#if subtitle}}
    {{title}}
    <span class="subtitle">{{subtitle}}</span>
  {{else}}
    {{name}}
  {{/if}}
</a>
